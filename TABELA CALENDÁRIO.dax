Calendário =

VAR DATAINICIAL  = DATE(2024, 1, 1)
VAR DATAFINAL  = TODAY()
VAR DATAS = SELECTCOLUMNS(CALENDAR(DATAINICIAL, DATAFINAL), "Data", [DATE])

VAR CALENDARIOS = ADDCOLUMNS(
    DATAS,
    "ANO", YEAR([Data]),
    "NºMES", MONTH([Data]),
    "MES", FORMAT([Data], "MMMM", "Pt-BR")    
)

RETURN CALENDARIOS